{
  "testEnvironment": "node",
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/importData/"
  ],
  "collectCoverageFrom": [
    "**/*.js",
    "!**/node_modules/**",
    "!**/tests/**",
    "!**/importData/test.js",
    "!jest.config.js"
  ]
}